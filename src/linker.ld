ENTRY(start)
phys = 0x00100000;
SECTIONS
{
  .text phys : AT(phys) {
    . = ALIGN(8192);
    KEEP(*(.multiboot))
    code = .;
    *(.text)
    *(.rodata)
    . = ALIGN(4096);
  }
  .data : AT(phys + (data - code))
  {
    data = .;
    *(.data)
    . = ALIGN(4096);
  }
  .bss : AT(phys + (bss - code))
  {
    bss = .;
    *(.bss)
    . = ALIGN(4096);
  }
  end = .;
}